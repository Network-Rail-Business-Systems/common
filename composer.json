{
    "name": "network-rail-business-systems/common",
    "description": "Provides common functionality for NRBS Laravel systems",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NetworkRailBusinessSystems\\Common\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tahreem Asghar",
            "email": "Tahreem.Asghar@networkrail.co.uk"
        },
        {
            "name": "Michael Dono",
            "email": "Michael.Dono2@networkrail.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "require" : {
        "php": "^8.3",
        "illuminate/support": "^12"
    },
    "require-dev" : {
        "brianium/paratest": "^7",
        "larastan/larastan": "^3",
        "laravel/pint": "^1",
        "networkrailbusinesssystems/badges": "^1",
        "orchestra/testbench": "^10",
        "phpunit/phpunit": "^12"
    },
    "config" : {
        "sort-packages" : true
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "NetworkRailBusinessSystems\\Common\\CommonServiceProvider"
            ]
        }
    }
}
